{
  "name": "react-selenium-testing",
  "version": "0.1.0",
  "description": "React selenium testing script",
  "main": "react-selenium-testing.js",
  "scripts": {
    "build": "babel react-selenium-testing.js -d Output/ --copy-files",
    "installpages": "npm install --prefix TestPages && npm run install-versions --prefix TestPages",
    "sauce": "node sauce_connect.js",
    "testpages": "npm start --prefix TestPages",
    "start": "npm-run-all -p -r sauce testpages"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
    "babel-plugin-transform-es3-property-literals": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "npm-run-all": "^4.1.3"
  },
  "dependencies": {
    "node-cmd": "^3.0.0",
    "sauce-connect-launcher": "^1.2.4",
    "uuid": "^3.3.2"
  }
}
